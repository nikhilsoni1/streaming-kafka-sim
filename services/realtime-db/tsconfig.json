{
    "compilerOptions": {
        "rootDir": ".",
        "outDir": "./dist",
        "target": "esnext",
        "module": "commonjs",
        "esModuleInterop": true,
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "removeComments": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["esnext", "esnext.asynciterable", "DOM"],
        "types": ["node", "lodash"],
        "baseUrl": "."
    }
}
